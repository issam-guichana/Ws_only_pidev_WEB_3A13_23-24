<form method="post" action="{{ path('app_question_delete', {'idQuest': question.idQuest,'idQuiz': idQuiz}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ question.idQuest) }}">
    <button class="btn btn-danger">Delete</button>
</form>
