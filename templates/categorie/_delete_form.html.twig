<form method="post" action="{{ path('app_categorie_delete', {'idCat': categorie.idCat}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.idCat) }}">
	<button class="btn">Supprimer</button>
</form>
